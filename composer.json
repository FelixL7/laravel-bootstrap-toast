{
    "name": "felixl7/laravel-bootstrap-toast",
    "description": "This is a simple Bootstrap Toast Package for Laravel.",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0"
    },
    "authors": [
        {
            "name": "Felix Lipinski",
            "email": "info@felixlipinski.de",
            "homepage": "https://felixlipinski.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "FelixL7\\Toast\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FelixL7\\Toast\\ToastServiceProvider"
            ],
            "aliases": {
                "Toaster": "FelixL7\\Toast\\Facades\\Toaster"
            }
        }
    }
}
